{
    "name": "visavi/rotor",
    "type": "project",
    "description": "Rotor",
    "keywords": ["php cms", "wap cms", "mobile cms"],
    "homepage": "https://visavi.net",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Alexander Grigorev",
            "email": "admin@visavi.net",
            "homepage": "https://visavi.net"
        },
        {
            "name": "Rotor Community",
            "homepage": "https://github.com/visavi/rotor/graphs/contributors"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "cbschuld/browser.php": "~1.9",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "google/recaptcha": "~1.2",
        "gregwar/captcha": "^1.1",
        "guzzlehttp/guzzle": "^7.3",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.46",
        "laravel/tinker": "^2.6",
        "nelexa/zip": "^3.3",
        "php-ffmpeg/php-ffmpeg": "~0.18",
        "visavi/cleanup": "^1.5"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "deployer/deployer": "^6.8",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12",
        "php-coveralls/php-coveralls": "^2.4",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "phpstan": "phpstan analyse --no-progress -vvv",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.1"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform": {
            "php": "7.3.0"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
